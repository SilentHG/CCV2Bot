{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
    "manifestVersion": "1.16",
    "version": "1.0.0",
    "id": "${{TEAMS_APP_ID}}",
    "packageName": "com.microsoft.teams.extension",
    "developer": {
        "name": "Teams App, Inc.",
        "websiteUrl": "https://www.example.com",
        "privacyUrl": "https://www.example.com/privacy",
        "termsOfUseUrl": "https://www.example.com/termofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "CCV2Bot-${{TEAMSFX_ENV}}",
        "full": "CCV2Helper"
    },
    "description": {
        "short": "Made to easier the CCV2 Deployment Process \n\n Made by Shakir Ali",
        "full": "Made to easier the CCV2 Deployment Process \n\n Made by Shakir Ali"
    },
    "accentColor": "#FFFFFF",
    "bots": [
        {
            "botId": "${{BOT_ID}}",
            "scopes": [
                "personal",
                "team",
                "groupchat"
            ],
            "supportsFiles": false,
            "isNotificationOnly": false,
            "commandLists": [
                {
                    "scopes": [
                        "personal",
                        "team",
                        "groupchat"
                    ],
                    "commands": [
                        {
                            "title": "help",
                            "description": "Help section about usage of commands"
                        },
                        {
                            "title": "getEnvironments",
                            "description": "Get All CCV2 Environments"
                        },
                        {
                            "title": "getBuildProgress",
                            "description": "Example Usage: getBuildProgress <buildId> | Description: Get Build Progress for a previously created Build"
                        },
                        {
                            "title": "createBuild",
                            "description": "Example Usage: createBuild <buildName> <branchName> | Description: Create a Build for a given branch"
                        },
                        {
                            "title": "createDeployment",
                            "description": "Example Usage: createDeployment <buildCode> <databaseUpdateMode> <environmentCode> <strategy>"
                        },
                        {
                            "title": "getDeploymentProgress",
                            "description": "Example Usage: getDeploymentProgress <deploymentId> | Description: Get Deployment Progress for a previously created Deployment"
                        }
                    ]
                }
            ]
        }
    ],
    "composeExtensions": [],
    "configurableTabs": [],
    "staticTabs": [],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}